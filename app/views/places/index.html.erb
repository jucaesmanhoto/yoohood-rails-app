<h1>Places#index</h1>
<p>Find me in app/views/places/index.html.erb</p>

<% @places.all.each do |place| %>
  <div class="card">
    <%= place.place_name %> | <%= place.latitude %> | <%= place.longitude %>
    | <%= place.address %> | <%= place.city %> | <%= place.country %> | <%= place.zip_code %> |
  </div>
<% end %>
